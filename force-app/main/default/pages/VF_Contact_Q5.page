<apex:page controller="contact_Q5Class">

    <apex:form >
        <apex:pageBlock >
            <apex:pageBlockSection >
                <apex:inputText Value="{!contact_Q5Class.firstname}"/>
                <apex:inputText value="{!contact_Q5Class.lastname}"/>
            </apex:pageBlockSection>
            
            <apex:pageBlockButtons >
                <apex:commandButton value="Save Contact" action="{!doSave}"/>
            </apex:pageBlockButtons>
            
            
        </apex:pageBlock>
    </apex:form>
  
</apex:page>